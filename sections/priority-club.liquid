<div class="home_priority_club">
    <div class="background">
        <img src="{{ section.settings.background_image | img_url:'master' }}" alt="{% if section.settings.background_image.alt %}{{section.settings.background_image.alt}}{% else %}Super Sleeper Pro - Priority club{% endif %}" loading="lazy" class="img-fluid priority_background" />
    </div>
    
    <div class="page-width">
      <div class="gridsection">
        <div class="pos_relative supersleeperimg">
          <img src="{{ section.settings.front_image | img_url:'master' }}" alt="{% if section.settings.front_image.alt %}{{ section.settings.front_image.alt }}{% else %}Super Sleeper Pro - Priority club{% endif %}" loading="lazy" class="img-fluid priority_logo" />
        </div>
  
        <div class="gridchild">
          {%- for block in section.blocks -%}
            <div class="gridchildcontent">
