{{ 'new-contact-form.scss.css' | asset_url | stylesheet_tag }}

<div id="contact_page">

  <section class="banner">
    <img src="https://cdn.shopify.com/s/files/1/0098/8915/3081/files/bed-with-book-and-flowers_2700x.jpg?v=1551157118"
      alt="contact us banner" class="img-fluid">
    <h1 class="page-title page-width">{{page.title}}</h1>
  </section>

  <div class="page-width">
    <div class="contents grid_contents">
      <div class="">
        <div class="successFormMsg" style="display: none;">
          Thanks for your message, we will get back to you soon!
        </div>
        <h3>Want to get in touch?</h3>
        <p class="drop_us_line">To speak to a friendly member of staff please contact us on
          <a href="tel:02 9387 4100">02 9387 4100</a>, alternatively you call fill out the below form and we will be in touch with you shortly.
        </p>
        <div class="pify_contact_form pos_relative">
          
          {% render 'pify-form' %}
        
        </div>

      </div>
      <div class="">
        <h3>REWARDS PROGRAM</h3>
        <p class="reward_program_message">
          <a href="#smile-home"><span
              class="fs__20 fwb lh__1 pr__10"><img
                src="https://cdn.sweettooth.io/v1/images/launcher_icons/crown.svg?color=%236568FE"
                width="45" /></span></a>
          <span class="">Earn more Priority-Points for different actions, and turn those Priority-Points into awesome rewards!</span>
        </p>
        <h3 class="mt__40">GIVE US A CALL</h3>
        <p class="reward_program_message">
          <span class="icon-call-in fs__40 fl">
            <svg xmlns="http://www.w3.org/2000/svg" width="45" height="45" fill="currentColor" class="bi bi-telephone-inbound" viewBox="0 0 16 16">
              <path d="M15.854.146a.5.5 0 0 1 0 .708L11.707 5H14.5a.5.5 0 0 1 0 1h-4a.5.5 0 0 1-.5-.5v-4a.5.5 0 0 1 1 0v2.793L15.146.146a.5.5 0 0 1 .708 0zm-12.2 1.182a.678.678 0 0 0-1.015-.063L1.605 2.3c-.483.484-.661 1.169-.45 1.77a17.568 17.568 0 0 0 4.168 6.608 17.569 17.569 0 0 0 6.608 4.168c.601.211 1.286.033 1.77-.45l1.034-1.034a.678.678 0 0 0-.063-1.015l-2.307-1.794a.678.678 0 0 0-.58-.122l-2.19.547a1.745 1.745 0 0 1-1.657-.459L5.482 8.062a1.745 1.745 0 0 1-.46-1.657l.548-2.19a.678.678 0 0 0-.122-.58L3.654 1.328zM1.884.511a1.745 1.745 0 0 1 2.612.163L6.29 2.98c.329.423.445.974.315 1.494l-.547 2.19a.678.678 0 0 0 .178.643l2.457 2.457a.678.678 0 0 0 .644.178l2.189-.547a1.745 1.745 0 0 1 1.494.315l2.306 1.794c.829.645.905 1.87.163 2.611l-1.034 1.034c-.74.74-1.846 1.065-2.877.702a18.634 18.634 0 0 1-7.01-4.42 18.634 18.634 0 0 1-4.42-7.009c-.362-1.03-.037-2.137.703-2.877L1.885.511z"/>
            </svg>
          </span>
          <span class="fs__20 fwb lh__1"><a href="tel:+61 2 9387 4100" class="call_us_now">+61 2 9387 4100</a><br>Call to speak to one of our helpful Customer Service Experts directly.</span>
        </p>

        <p class="reward_program_message address_location">
          <span class="icon-call-in fs__40 fl">
            <svg xmlns="http://www.w3.org/2000/svg" width="45" height="45" fill="currentColor" class="bi bi-geo-alt" viewBox="0 0 16 16">
              <path d="M12.166 8.94c-.524 1.062-1.234 2.12-1.96 3.07A31.493 31.493 0 0 1 8 14.58a31.481 31.481 0 0 1-2.206-2.57c-.726-.95-1.436-2.008-1.96-3.07C3.304 7.867 3 6.862 3 6a5 5 0 0 1 10 0c0 .862-.305 1.867-.834 2.94zM8 16s6-5.686 6-10A6 6 0 0 0 2 6c0 4.314 6 10 6 10z"/>
              <path d="M8 8a2 2 0 1 1 0-4 2 2 0 0 1 0 4zm0 1a3 3 0 1 0 0-6 3 3 0 0 0 0 6z"/>
            </svg>
          </span>
          <span class="fs__20 fwb lh__1"><a href="#" class="call_us_now">Address</a><br>Suite 301, 209 Oxford St, Bondi Junction, NSW 2022</span>
        </p>
      </div>
    </div>
  </div>

</div>


<style>
.drop_us_line a{
  
}

.toggler-wrapper button.icon-bubble{
  display: none!important;
  z-index: -1;
}
</style>