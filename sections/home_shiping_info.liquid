<style>
    .container_home_message_with_review ul{
        list-style-type: none;
    }

    #home_message_with_review{
        background: #fff;
        box-shadow: 0 -10px 40px #00000036;
        padding: 1.5em 0 0;
        border-radius: 10px;
        position: relative;
        z-index: 1;
        margin-top: -2em;
    }

    .container_home_message_with_review svg{
        width: clamp(35px,6vw,50px);
        height: clamp(35px,6vw,50px);
    }

    .container_home_message_with_review .flex{
        display: flex;
        gap: 1em;
      justify-content: center;
    }

    .container_home_message_with_review p{
        font-size: 13px;
        margin-top: 4px;
        margin-bottom: 0;
    }

    .container_home_message_with_review h3{
        margin: 0;
        font-size: 16px;
        text-align: left; 
        line-height: 1.4;
    }

    .container_home_message_with_review .first_row{
        display: grid;
        grid-template-columns: repeat(4, 1fr);
        justify-content: space-between;
        gap: 8px;
        row-gap: 12px;
        padding: 0 1em 1.5em;
        background: #fff;
    }
    

    @media (max-width: 800px){
        .container_home_message_with_review .first_row{
            grid-template-columns: repeat(2, 1fr);
        }

        .container_home_message_with_review .flex{
            justify-content: flex-start;
        }
    }

    @media (max-width: 600px){
        .container_home_message_with_review .first_row{
            grid-template-columns: 1fr;
           padding-bottom: 12px;
        }

      .container_home_message_with_review .flex{
        justify-content: flex-start;
      }
    }


    .container_home_message_with_review .custom_review{
        background: #ff3271;
        color: #fff;
        text-align: center;
        font-size: 15px;
        line-height: 15px;
        padding: 5px 10px;
        border-radius: 0 0 10px 10px;
        -webkit-border-radius: 0 0 10px 10px;
        -moz-border-radius: 0 0 10px 10px;
        box-shadow: 0 0 20px #fb326e99;
        font-weight: 300;
        font-size: 15px;
        display: flex;
        justify-content: center;
        gap: 5px;
        min-height: 39.5px;
        align-items: center;
        z-index: -1;
        position: relative;
    }

    .container_home_message_with_review .custom_review a{
        position: relative;
        bottom: inherit;
        right: inherit; 
        color: #fff;
      display: block;
    }

    .container_home_message_with_review a.okendositereview star-review{
        color: #fff;
        position: relative;
    }

  @media (max-width: 1120px){
    .container_home_message_with_review .first_row br{
      display: none;
    }
  }

</style>

